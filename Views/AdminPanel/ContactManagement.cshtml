@model KuaforRandevu2.Models.Entities.Contact
@{
    Layout = "_AdminLayout";
}
<div class="container mt-5">
    <div class="row">
        <div class="col-md-12">
            @using (Html.BeginForm("ContactManagement", "AdminPanel", null, FormMethod.Post, true, new { @class = "form-group" }))
            {
                @Html.LabelFor(m => m.Instagram, "Instagram");
                @Html.TextBoxFor(m => m.Instagram, new { @class = "form-control" });
                @Html.LabelFor(m => m.Facebook, "Facebook");
                @Html.TextBoxFor(m => m.Facebook, new { @class = "form-control" });
                @Html.LabelFor(m => m.GooglePlus, "Google+");
                @Html.TextBoxFor(m => m.GooglePlus, new { @class = "form-control" });
                @Html.LabelFor(m => m.Email, "E-Mail");
                @Html.TextBoxFor(m => m.Email, new { @class = "form-control" });
                @Html.LabelFor(m => m.Phone, "Phone");
                @Html.TextBoxFor(m => m.Phone, new { @class = "form-control" });
                @Html.LabelFor(m => m.Address, "Address");
                @Html.TextBoxFor(m => m.Address, new { @class = "form-control" });
                <button class="btn btn-primary" type="submit">Update</button>
            }
            <a class="btn btn-secondary" href="~/AdminPanel">Admin Home</a>
        </div>
    </div>
</div>

